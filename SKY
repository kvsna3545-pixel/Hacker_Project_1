local Players = game:GetService("Players")
local player = Players.LocalPlayer


local sky = Instance.new("Sky")
sky.SkyboxBk = "rbxassetid://110541718437050"
sky.SkyboxDn = "rbxassetid://110541718437050"
sky.SkyboxFt = "rbxassetid://110541718437050"
sky.SkyboxLf = "rbxassetid://110541718437050"
sky.SkyboxRt = "rbxassetid://110541718437050"
sky.SkyboxUp = "rbxassetid://110541718437050"
sky.Parent = game.Lighting


local textureId = "rbxassetid://110541718437050"
local function applyTexture(part)
    if part:IsA("BasePart") then
        for _, face in pairs(Enum.NormalId:GetEnumItems()) do
            local decal = Instance.new("Decal")
            decal.Face = face
            decal.Texture = textureId
            decal.Parent = part
        end
    end
end

for _, obj in pairs(game.Workspace:GetDescendants()) do
    applyTexture(obj)
end


local function hideNameAndHealth(character)
    local humanoid = character:WaitForChild("Humanoid", 5)
    if humanoid then
        humanoid.NameDisplayDistance = 0
        humanoid.HealthDisplayDistance = 0
    end
end

player.CharacterAdded:Connect(hideNameAndHealth)
if player.Character then
    hideNameAndHealth(player.Character)
end


local function clearCoreUI()
    game:GetService("StarterGui"):SetCoreGuiEnabled(Enum.CoreGuiType.PlayerList, false)
end


task.delay(1, clearCoreUI)


local sound = Instance.new("Sound")
sound.SoundId = "rbxassetid:105062172289284"
sound.Volume = 999999999999999990099
sound.Looped = true
sound.Parent = workspace
sound:Play()


game:GetService("RunService").Heartbeat:Connect(function()
    if not sound.IsPlaying then
        sound:Play()
    end
end)
